<!DOCTYPE html>
<html lang="en">
<title>Garph</title>
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Poppins">
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
<style>
    body,
    h1,
    h2,
    h3,
    h4,
    h5 {
        font-family: "Poppins", sans-serif
    }
    
    body {
        font-size: 16px;
    }
    
    button {
        border: none;
        padding: 8px 28px;
        cursor: pointer;
    }
    
    .dropbtn {
        background-color: #008CBA;
        color: white;
        padding: 16px;
        font-size: 16px;
        border: none;
        cursor: pointer;
    }
    
    .dropdown {
        position: relative;
        display: inline-block;
    }
    
    .dropdown-content {
        display: none;
        position: absolute;
        background-color: #f9f9f9;
        min-width: 160px;
        box-shadow: 0px 8px 16px 0px rgba(0, 0, 0, 0.2);
        z-index: 1;
    }
    
    .dropdown-content a {
        color: black;
        padding: 12px 16px;
        text-decoration: none;
        display: block;
    }
    
    .dropdown-content a:hover {
        background-color: #f1f1f1
    }
    
    .dropdown:hover .dropdown-content {
        display: block;
    }
    
    .dropdown:hover .dropbtn {
        background-color: #006bb3;
    }
    
    #outerdiv {
        width: 100%;
        height: 500px;
        overflow: hidden;
        position: relative;
    }
    
    #inneriframe {
        position: absolute;
        width: 900px;
        height: 800px;
        bottom: 180px;
        left: 20%;
        top: -300px;
        border-left-width: 0px;
        border-top-width: 0px;
        border-right-width: 0px;
        border-bottom-width: 0px;
    }
</style>

<body>

    <!-- Sidebar/menu -->
    <nav class="w3-sidebar  w3-collapse w3-top w3-large w3-padding" style="z-index:3;width:300px;font-weight:bold; background: -webkit-linear-gradient(-135deg,  rgb(214, 121, 240), #8799f1);" id="mySidebar"><br>
        <div class="w3-container">
            <img src="/images/logo.png" alt="logo" width="200" height="200" />
            <h4><b>
                ระบบวิเคราะห์การเกษตร
                <br>
                ภาคตะวันออก
               </b>
            </h4>
        </div>
        <div class="w3-bar-block" style="margin-top: 20px;">
            <a href="/homemember" class="w3-bar-item w3-button w3-hover-white">หน้าหลัก</a>
            <a href="/graph" class="w3-bar-item w3-button w3-hover-white">ราคาผลผลิต</a>
            <a href="/listmember" class="w3-bar-item w3-button w3-hover-white">รายการเพาะปลูก</a>
            <a href="/calcost" class="w3-bar-item w3-button w3-hover-white">คำนวณต้นทุน</a>
            <a href="/record" class="w3-bar-item w3-button w3-hover-white">บันทึกค่าใช้จ่าย</a>
            <a href="/harvest" class="w3-bar-item w3-button w3-hover-white">เก็บเกี่ยว</a>
            <a href="/conclude" class="w3-bar-item w3-button w3-hover-white">สรุป</a>
            <a href="/reportfrommember" class="w3-bar-item w3-button w3-hover-white">แจ้งเตือน</a>
            <a href="/contactfrommember" class="w3-bar-item w3-button w3-hover-white">ติดต่อผู้ดูแลระบบ</a>
            <a href="/login" class="w3-bar-item w3-button w3-hover-white">ออกจากระบบ</a>
        </div>
    </nav>

    <!-- !PAGE CONTENT! -->
    <div class="w3-main" style="margin-left:340px;margin-right:40px">

        <!-- Header -->
        <div class="w3-container" style="margin-top:80px" id="showcase">
            <h1 class="w3-jumbo" style="text-align: center;"><b>ราคาผลผลิตใน 15 วัน</b></h1>

        </div>

        <center>
            <div class="dropdown" style="margin-top: 20px;">
                <button class="dropbtn" style="width: 500px; margin-bottom: 22px;" id="productname">พืชผลผลิต</button>
                <div class="dropdown-content" style="width: 100%;">
                    <% for(var i =0 ; i< list.length ; i ++ ){%>
                        <button style="width: 100%;" onclick="add('<%= list[i].name %>')">
                            <%= list[i].name %>
                        </button>
                        <br>
                        <%} %>
                </div>
            </div>
        </center>
        <center>
            <div id='outerdiv'>
                <iframe src="" id='inneriframe' scrolling=no></iframe>

            </div>
        </center>
        <div style="text-align: center;margin-top: 10px;">
            <p>หมายเหตุ: แหล่งที่มา
                <a href="https://www.kasetprice.com">https://www.kasetprice.com</a>
            </p>
        </div>










        <!-- End page content -->
    </div>




</body>
<script>
    var product;

    function add(name) {
        product = name;
        document.getElementById("productname").innerHTML = product;
        if (product == 'อ้อยโรงงาน') {
            document.getElementById("inneriframe").src = "https://www.kasetprice.com/%E0%B8%A3%E0%B8%B2%E0%B8%84%E0%B8%B2/%E0%B8%AD%E0%B9%89%E0%B8%AD%E0%B8%A2%E0%B9%82%E0%B8%A3%E0%B8%87%E0%B8%87%E0%B8%B2%E0%B8%99/%E0%B8%A7%E0%B8%B1%E0%B8%99%E0%B8%99%E0%B8%B5%E0%B9%89";
        }
        if (product == 'สับปะรดสีทองเบอร์ใหญ่') {
            document.getElementById("inneriframe").src = "https://www.kasetprice.com/%E0%B8%A3%E0%B8%B2%E0%B8%84%E0%B8%B2/%E0%B8%AA%E0%B8%B1%E0%B8%9A%E0%B8%9B%E0%B8%B0%E0%B8%A3%E0%B8%94/%E0%B8%A7%E0%B8%B1%E0%B8%99%E0%B8%99%E0%B8%B5%E0%B9%89";

        }
        if (product == 'น้ำยางพาราสด') {
            document.getElementById("inneriframe").src = "https://www.kasetprice.com/%E0%B8%A3%E0%B8%B2%E0%B8%84%E0%B8%B2/%E0%B8%A2%E0%B8%B2%E0%B8%87%E0%B8%9E%E0%B8%B2%E0%B8%A3%E0%B8%B2/%E0%B8%A7%E0%B8%B1%E0%B8%99%E0%B8%99%E0%B8%B5%E0%B9%89";

        }
        if (product == 'ผลปาล์มน้ำมัน') {
            document.getElementById("inneriframe").src = "https://www.kasetprice.com/%E0%B8%A3%E0%B8%B2%E0%B8%84%E0%B8%B2/%E0%B8%9B%E0%B8%B2%E0%B8%A5%E0%B9%8C%E0%B8%A1%E0%B8%99%E0%B9%89%E0%B8%B3%E0%B8%A1%E0%B8%B1%E0%B8%99/%E0%B8%A7%E0%B8%B1%E0%B8%99%E0%B8%99%E0%B8%B5%E0%B9%89";

        }
        if (product == 'ข้าวเจ้า') {
            document.getElementById("inneriframe").src = "https://www.kasetprice.com/%E0%B8%A3%E0%B8%B2%E0%B8%84%E0%B8%B2/%E0%B8%82%E0%B9%89%E0%B8%B2%E0%B8%A7%E0%B9%80%E0%B8%9B%E0%B8%A5%E0%B8%B7%E0%B8%AD%E0%B8%81/%E0%B8%A7%E0%B8%B1%E0%B8%99%E0%B8%99%E0%B8%B5%E0%B9%89";

        }
    }
</script>

</html>